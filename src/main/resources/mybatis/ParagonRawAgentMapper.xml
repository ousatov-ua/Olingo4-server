        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.olus.olingo4.nnmrls.dao.mappers.ParagonRawAgentMapper">
    <select id="selectParagonRawAgents" resultType="map" resultSetType="SCROLL_SENSITIVE">
        select *
        from ParagonRawAgent LIMIT #{param1}, #{param2}
    </select>

    <select id="selectParagonRawAgentById" resultType="map">
        select *
        from ParagonRawAgent where User_Code=#{id}
    </select>

