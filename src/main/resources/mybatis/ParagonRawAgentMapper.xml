        (User_Code,Active,Goomzee,NRDS_ID,Agent_Url,Agent_Zip,Team_Name,Agent_City,Agent_Type,Date_Added,Agent_Email,Agent_State,Status_Date,Team_E_mail,Update_Date,Agent_Status,Photographer,Board_User_ID,Agent_Address2,Agent_Mail_Zip,Multiple_Logon,Security_Level,Agent_Last_Name,Agent_Mail_City,Agent_Office_ID,Update_By_Agent,Agent_Birth_Date,Agent_First_Name,Agent_Identifier,Agent_License_ID,Agent_Logon_Name,Agent_Mail_State,Agent_Salutation,MLStimate_Active,User_Photos_Code,Agent_Designation,Agent_License_Date,Agent_Mail_Address2,Agent_Phone1_Number,Agent_Phone2_Number,Agent_Phone3_Number,Agent_Phone4_Number,Agent_Phone5_Number,Primary_Association,Agent_Middle_Initial,Agent_Mail_Preference,Agent_Mail_Street_Name,Agent_Phone1_CountryId,Agent_Phone1_Extension,Agent_Phone2_CountryId,Agent_Phone2_Extension,Agent_Phone3_CountryId,Agent_Phone3_Extension,Agent_Phone4_CountryId,Agent_Phone4_Extension,Agent_Phone5_CountryId,Agent_Phone5_Extension,UPLogo_image_Timestamp,UPAgent_image_Timestamp,Agent_Phone1_Description,Agent_Phone2_Description,Agent_Phone3_Description,Agent_Phone4_Description,Agent_Phone5_Description,State_Agent_Is_Member_Of,Agent_Address_Street_Name)
