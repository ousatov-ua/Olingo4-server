<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.olus.olingo4.nnmrls.dao.mappers.ParagonRawListingRemarksMapper">
    <select id="selectParagonRawListingRemarks" resultType="map" resultSetType="SCROLL_SENSITIVE">
        select *
        from ParagonRawListingRemarks LIMIT #{param1}, #{param2}
    </select>

    <select id="selectParagonRawListingRemarksById" resultType="map">
        select *
        from ParagonRawListingRemarks where Mls_Number=#{id}
    </select>
</mapper>