<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.olus.olingo4.nnmrls.dao.mappers.ParagonRawOfficeMapper">
    <select id="selectParagonRawOffices" resultType="map">
        select *
        from ParagonRawOffice
    </select>

    <select id="selectParagonRawOfficeById" resultType="map">
        select *
        from ParagonRawOffice where Office_Abbreviation=#{id}
    </select>
</mapper>